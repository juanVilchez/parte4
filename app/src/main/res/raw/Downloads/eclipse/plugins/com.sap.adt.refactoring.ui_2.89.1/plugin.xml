<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

  <extension-point id="quickfixHandlers" name="%extension.point.name.quickfixHandlers_xgrp" schema="schema/quickfixHandler.exsd"/>
  
  <extension
        point="org.eclipse.ui.contexts">
     <context
           id="com.sap.adt.refactoring.ui.refactoringViewContext"
           name="%context.name.RefactoringView_xtxt"
           parentId="org.eclipse.ui.contexts.window">
     </context>
  </extension>

  <!-- Commands -->
  <extension
       point="org.eclipse.ui.commands">
      <command
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            helpContextId="com.sap.adt.abapdev.guide.doc.user.source_extract_method_action_context"
            id="com.sap.adt.tools.core.ui.extract.method"
            name="%command.name.extract.method_xmit">
      </command>
      <command
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            helpContextId="com.sap.adt.abapdev.guide.doc.user.source_rename_action_context"
            id="com.sap.adt.tools.core.ui.quickfix.renameInFile"
            name="%command.name.quickfix.renameInFile_xmit">
      </command>
      <command
            defaultHandler="com.sap.adt.refactoring.ui.internal.op.rename.RenameHandler"
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            helpContextId="com.sap.adt.abapdev.guide.doc.user.source_rename_action_context"
            id="com.sap.adt.tools.core.ui.rename"
            name="%command.name.rename_xmit">
      </command>
      <command
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            id="com.sap.adt.tools.core.ui.quickfix.assignToLocalVariable"
            name="%command.name.quickfix.assignToLocalVariable_xmit">
      </command>
      <command
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            id="com.sap.adt.tools.core.ui.quickfix.delete.member"
            name="%command.name.quickfix.delete.member_xmit">
      </command>
      <command
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            id="com.sap.adt.tools.core.ui.quickfix.assignToAttribute"
            name="%command.name.quickfix.assignToAttribute_xmit">
      </command>
      <command
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            id="com.sap.adt.tools.core.ui.quickfix.extractLocalVariable"
            name="%command.name.quickfix.extractLocalVariable_xmit">
      </command>
      <command
            categoryId="com.sap.adt.tools.abapsource.ui.source.category"
            id="com.sap.adt.refactoring.ui.ApplyProposal"
            name="%command.name.refactoring.applyProposal_xmit">
      </command>
     <command
           categoryId="com.sap.adt.tools.abapsource.ui.source.category"
           id="com.sap.adt.refactoring.ui.showInQuickAssistView"
           name="%command.name.refactoring.showInQuickAssistView_xmit">
     </command>
     <command
           categoryId="com.sap.adt.tools.abapsource.ui.source.category"
           id="com.sap.adt.refactoring.view.navigateTo"
           name="%command.name.refactoring.navigateTo_xmit">
     </command>
     <command
           id="com.sap.adt.refactoring.ui.changePackageAssignment"
           name="%command.name.refactoring.changePackageAssignment_xmit">
     </command>
  </extension>

  <!-- Key Bindings -->
  <extension
       point="org.eclipse.ui.bindings">
      <key
            commandId="com.sap.adt.tools.core.ui.extract.method"
            contextId="com.sap.adt.tools.abapsource.ui.abapCode"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+M">
      </key>
      <key
            commandId="com.sap.adt.tools.core.ui.quickfix.renameInFile"
            contextId="com.sap.adt.tools.abapsource.ui.abapCode"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+2 R">
      </key>
      <key
            commandId="com.sap.adt.tools.core.ui.quickfix.assignToLocalVariable"
            contextId="com.sap.adt.tools.abapsource.ui.abapCode"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+2 L">
      </key>
      <key
            commandId="com.sap.adt.tools.core.ui.quickfix.assignToAttribute"
            contextId="com.sap.adt.tools.abapsource.ui.abapCode"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+2 F">
      </key>
      <key
            commandId="com.sap.adt.tools.core.ui.quickfix.extractLocalVariable"
            contextId="com.sap.adt.tools.abapsource.ui.abapCode"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+L">
      </key>
      <key
            commandId="com.sap.adt.tools.core.ui.rename"
            contextId="com.sap.adt.tools.abapsource.ui.abapCode"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+R">
      </key>
      <key
            commandId="com.sap.adt.tools.core.ui.rename"
            contextId="com.sap.adt.tools.abapsource.ui.editorOutlineContext"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+R">
      </key>
      <key
            commandId="com.sap.adt.tools.core.ui.quickfix.delete.member"
            contextId="com.sap.adt.tools.abapsource.ui.editorOutlineContext"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="DEL">
      </key>
      <key
            commandId="com.sap.adt.refactoring.ui.ApplyProposal"
            contextId="com.sap.adt.refactoring.ui.refactoringViewContext"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CR">
      </key>
      <key
            commandId="com.sap.adt.refactoring.ui.showInQuickAssistView"
            contextId="com.sap.adt.tools.abapsource.ui.abapCode"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+1">
      </key>
      
  </extension>

  <!-- Command Handlers -->
  <extension
       point="org.eclipse.ui.handlers">
	 <handler
	       class="com.sap.adt.refactoring.ui.internal.op.rename.RenameHandler"
	       commandId="com.sap.adt.tools.core.ui.rename">
	       <activeWhen>
	       <iterate operator="or">
	         <or>
	         	<with
	                variable="activeContexts">
	             <iterate
	                   ifEmpty="false"
	                   operator="or">
                 <and>
                    <equals
                          value="com.sap.adt.tools.core.ui.abap">
                    </equals>
                 </and>
	             </iterate>
	          </with>
           <instanceof
                 value="com.sap.adt.tools.abapsource.ui.sources.outline.IAdtOutlineTreeNode">
           </instanceof>
	        </or>
	       </iterate>	  
	       </activeWhen>
	 </handler>
	 <handler
	       class="com.sap.adt.refactoring.ui.internal.quickfix.AbapSourceDirectQuickfixCommandHandler"
	       commandId="com.sap.adt.tools.core.ui.quickfix.delete.member">
	       <activeWhen>
	       		<iterate operator="or">
               <instanceof
                     value="com.sap.adt.tools.abapsource.ui.sources.outline.IAdtOutlineTreeNode">
               </instanceof>
               <test
                      property="com.sap.adt.compatibility.isNodeAvailable"
                      value="COM.SAP.ADT.QUICKFIXES.deleteMembers">
                </test>
	       		</iterate>
	       </activeWhen>
	 </handler>
	 <handler
	       class="com.sap.adt.refactoring.ui.internal.op.extractmethod.ExtractMethodHandler"
	       commandId="com.sap.adt.tools.core.ui.extract.method">
	       <activeWhen>
	          <with
	                variable="activeContexts">
	             <iterate
	                   ifEmpty="false"
	                   operator="or">
	                <equals
	                      value="com.sap.adt.tools.abapsource.ui.abapCode">
	                </equals>
	             </iterate>
	          </with>
	       </activeWhen>       
	 </handler>
     <handler
          class="com.sap.adt.refactoring.ui.internal.quickfix.AbapSourceDirectQuickfixCommandHandler"
          commandId="com.sap.adt.tools.core.ui.quickfix.renameInFile">
       <activeWhen>
          <with
                variable="activeContexts">
             <iterate
                   ifEmpty="false"
                   operator="or">
                <equals
                      value="com.sap.adt.tools.abapsource.ui.abapCode">
                </equals>
             </iterate>
          </with>
       </activeWhen>
     </handler>
     <handler
          class="com.sap.adt.refactoring.ui.internal.quickfix.AbapSourceDirectQuickfixCommandHandler"
          commandId="com.sap.adt.tools.core.ui.quickfix.assignToLocalVariable">
       <activeWhen>
          <with
                variable="activeContexts">
             <iterate
                   ifEmpty="false"
                   operator="or">
                <equals
                      value="com.sap.adt.tools.abapsource.ui.abapCode">
                </equals>
             </iterate>
          </with>
       </activeWhen>
     </handler>
     <handler
          class="com.sap.adt.refactoring.ui.internal.quickfix.AbapSourceDirectQuickfixCommandHandler"
          commandId="com.sap.adt.tools.core.ui.quickfix.assignToAttribute">
       <activeWhen>
          <with
                variable="activeContexts">
             <iterate
                   ifEmpty="false"
                   operator="or">
                <equals
                      value="com.sap.adt.tools.abapsource.ui.abapCode">
                </equals>
             </iterate>
          </with>
       </activeWhen>
     </handler>
     <handler
          class="com.sap.adt.refactoring.ui.internal.quickfix.AbapSourceDirectQuickfixCommandHandler"
          commandId="com.sap.adt.tools.core.ui.quickfix.extractLocalVariable">
       <activeWhen>
          <with
                variable="activeContexts">
             <iterate
                   ifEmpty="false"
                   operator="or">
                <equals
                      value="com.sap.adt.tools.abapsource.ui.abapCode">
                </equals>
             </iterate>
          </with>
       </activeWhen>
     </handler>
     <handler
          class="com.sap.adt.refactoring.ui.internal.views.handlers.ApplyRefactoringHandler"
          commandId="com.sap.adt.refactoring.ui.ApplyProposal">
       <activeWhen>
	       	<iterate operator="or">
               <instanceof
                     value="com.sap.adt.refactoring.ui.internal.views.IAdtRefactoringProposal">
               </instanceof>
	       	</iterate>
       </activeWhen>
     </handler>
     <handler
           class="com.sap.adt.refactoring.ui.internal.handlers.OpenQuickAssistViewHandler"
           commandId="com.sap.adt.refactoring.ui.showInQuickAssistView">
     </handler>
     <handler
           class="com.sap.adt.refactoring.ui.internal.views.handlers.NavigateHandler"
           commandId="com.sap.adt.refactoring.view.navigateTo">
        <enabledWhen>
           <iterate>
              <or>
                 <instanceof
                       value="com.sap.adt.refactoring.ui.internal.views.IQuickfixResult">
                 </instanceof>
                 <instanceof
                       value="com.sap.adt.refactoring.ui.internal.views.IRefactoringDelta">
                 </instanceof>
              </or>
           </iterate>
        </enabledWhen>
     </handler>
     <handler
           class="com.sap.adt.refactoring.ui.internal.op.changepackage.ChangePackageHandler"
           commandId="com.sap.adt.refactoring.ui.changePackageAssignment">
        <activeWhen>
           <and>
           <iterate
                 operator="or">
              <or>
                 <with
                       variable="activeContexts">
                    <iterate
                          ifEmpty="false"
                          operator="or">
                       <and>
                          <equals
                                value="com.sap.adt.tools.core.ui.abap">
                          </equals>
                       </and>
                    </iterate>
                 </with>
                 <instanceof
                       value="com.sap.adt.tools.abapsource.ui.sources.outline.IAdtOutlineTreeNode">
                 </instanceof>
              </or>
           </iterate>
           </and>
        </activeWhen>
     </handler>
  </extension>

  <!-- Menus -->
  <extension
       point="org.eclipse.ui.menus"> 
      <menuContribution
          locationURI="menu:com.sap.adt.tools.abapsource.ui.source?after=editGroup">
      	<command
             commandId="com.sap.adt.tools.core.ui.rename"
             style="push">
              <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activePart">
                  <test
                      args="com.sap.adt.tools.core.ui.rename"
                      forcePluginActivation="true"
                      property="com.sap.adt.tools.abapsource.ui.editormenu.shouldShowContextMenu">
                </test>
                </with>
                </visibleWhen>
       	</command>
      </menuContribution>
      <menuContribution
            locationURI="popup:#EditorContext?after=com.sap.adt.ris.whereused.ui.callWhereUsed">
         <command
               commandId="com.sap.adt.tools.core.ui.rename"
               style="push">
                 <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activePart">
                  <test
                        args="com.sap.adt.tools.core.ui.rename"
                        forcePluginActivation="true"
                        property="com.sap.adt.tools.abapsource.ui.editormenu.shouldShowContextMenu">
                </test>

               </with>
            </visibleWhen>
         </command>
         <command
               commandId="com.sap.adt.refactoring.ui.changePackageAssignment"
               icon="icons/obj/correction_move.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <and>
               <with
                     variable="activePart">
                  <test
                        property="com.sap.adt.compatibility.isResourceAvailable"
                        value="http://www.sap.com/adt/categories/refactorings#changepackage">
               	  </test>
                  <test
                        args="com.sap.adt.refactoring.ui.changePackageAssignment"
                        property="com.sap.adt.tools.abapsource.ui.editormenu.shouldShowContextMenu">
                  </test>
               </with>
               </and>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
          locationURI="menu:com.sap.adt.tools.abapsource.ui.source?after=editGroup">
      	<command
             commandId="com.sap.adt.tools.core.ui.extract.method"
             style="push">
              <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activePart">
                  <test
                      args="com.sap.adt.tools.core.ui.extract.method"
                      forcePluginActivation="true"
                      property="com.sap.adt.tools.abapsource.ui.editormenu.shouldShowContextMenu"
                     >
                </test>

               </with>
            </visibleWhen>
       	</command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sap.adt.tools.core.ui.outline?before=additions">
         <command
               commandId="com.sap.adt.tools.core.ui.rename"
               style="push">
               <visibleWhen>
	               <count value="1"/>
               </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sap.adt.tools.core.ui.outline?endof=group.edit">
         <command
               commandId="com.sap.adt.tools.core.ui.quickfix.delete.member"
               icon="icons/obj/delete_obj.png"
               style="push">
           <visibleWhen>
               <and>
               <count value="1"/>
               <iterate
                     operator="or">
                  <or>
                     <instanceof
                           value="com.sap.adt.tools.abapsource.ui.sources.outline.IAdtOutlineTreeNode">
                     </instanceof></or>
                  <test
                        property="com.sap.adt.compatibility.isNodeAvailable"
                        value="COM.SAP.ADT.QUICKFIXES.deleteMembers">
                  </test>
                  <and>
                  	<or>
                  		<test
                        	property="com.sap.adt.tools.core.objectTypes.isClifMember">
                  		</test>
                  		<test
                        	property="com.sap.adt.tools.core.objectTypes.isLocalClif">
                  		</test>
                  	</or>
                  </and>
               </iterate>
              </and>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?before=additions">
         <command
               commandId="com.sap.adt.tools.core.ui.rename"
               style="push">
            <visibleWhen>
               <and>
               <count value="1"/>
               <iterate
                     operator="or">
                  <or>
                     <instanceof
                           value="com.sap.adt.tools.abapsource.ui.sources.outline.IAdtOutlineTreeNode">
                     </instanceof></or>
               </iterate>
               </and>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sap.adt.refactoring.ui.RefactoringView">
         <command
               commandId="com.sap.adt.refactoring.ui.ApplyProposal"
               style="push">
            <visibleWhen>
               <and>
                  <count value="1"/>
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <and>
                        <instanceof
                              value="com.sap.adt.refactoring.ui.internal.views.IAdtRefactoringProposal">
                        </instanceof>
                        <test
                              property="com.sap.adt.refactoring.proposalIsApplicable">
                        </test>
                     </and>
                  </iterate>
               </and>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=group.search">
         <command
               commandId="com.sap.adt.refactoring.ui.changePackageAssignment"
               icon="icons/obj/correction_move.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <test
                     	   property="com.sap.adt.compatibility.isResourceAvailable"
                      	   value="http://www.sap.com/adt/categories/refactorings#changepackage">
                     </test>
                     <adapt
                           type="com.sap.adt.tools.core.IAdtObjectReference">
                     </adapt>
                     <or>
                        <test
                              property="com.sap.adt.projectexplorer.node.isSubtype"
                              value="false">
                        </test>
                        <and>
                        <test
                              property="com.sap.adt.tools.core.objectTypes.hasCapability"
                              value="com.sap.adt.tools.core.capabilities.supportsActivation">
                        </test>
                        <not>
                           <test
                                 property="com.sap.adt.tools.core.objectTypes.hasType"
                                 value="com.sap.adt.functions.fmodules">
                           </test>
                        </not>
                        </and>
                     </or>
                     <and>
                        <not>
                           <instanceof
                                 value="com.sap.adt.projectexplorer.ui.internal.node.AbapRepositoryPackageNode">
                           </instanceof>
                        </not>
                        <not>
                           <instanceof
                                 value="com.sap.adt.projectexplorer.ui.internal.virtualfolders.VirtualFolderNode">
                           </instanceof>
                        </not>                        
                     </and>
                  </iterate>
               </and>
            </visibleWhen>
         </command>
      </menuContribution>
  </extension>
 
  <!-- Property Testers -->
  <extension
       point="org.eclipse.core.expressions.propertyTesters">
    <propertyTester
          class="com.sap.adt.refactoring.ui.internal.DummyPropertyTester"
          id="com.sap.adt.refactoring.ui.dummyPropertyTester"
          namespace="com.sap.adt.refactoring.ui"
          properties="dummy"
          type="java.lang.Object">
    </propertyTester>
    <propertyTester
          class="com.sap.adt.refactoring.ui.internal.views.RefactoringViewPropertyTester"
          id="com.sap.adt.refactoring.ui.refactoringProposalPropertyTester"
          namespace="com.sap.adt.refactoring"
          properties="proposalIsApplicable"
          type="com.sap.adt.refactoring.ui.internal.views.IQuickAssistTreeNode">
    </propertyTester>
  </extension>
 
  <!-- Quickfix Handlers -->
  <extension
       point="com.sap.adt.refactoring.ui.quickfixHandlers">
    <quickfixHandler
          resourceUriPattern=".*/qf_create_method"
          class="com.sap.adt.refactoring.ui.internal.quickfix.createmethod.CreateMethodQuickfixHandler"
          name="Create Method">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/qf_create_method_from_call"
          class="com.sap.adt.refactoring.ui.internal.quickfix.createmethod.CreateMethodQuickfixHandler"
          name="Create Method From Call">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/qf_create_method_def"
          class="com.sap.adt.refactoring.ui.internal.quickfix.createmethod.CreateMethodQuickfixHandler"
          name="Create Method Definition">
    </quickfixHandler>    
    <quickfixHandler
          resourceUriPattern=".*/qf_command_rename"
          class="com.sap.adt.refactoring.ui.internal.quickfix.CommandQuickfixHandler"
          name="Rename Command">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/qf_command_create_object"
          class="com.sap.adt.refactoring.ui.internal.quickfix.CreateObjectQuickfixHandler"
          name="Create Object">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/qf_extract_method"
          class="com.sap.adt.refactoring.ui.internal.quickfix.CommandQuickfixHandler"
          name="Extract Method">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/generate_constructor"
          class="com.sap.adt.refactoring.ui.internal.generatecontructor.GenerateContructorHandler"
          name="Generate Contructor">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/generate_factory_method"
          class="com.sap.adt.refactoring.ui.internal.generatecontructor.GenerateContructorHandler"
          name="Generate Factory Method">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/create_text_symbol"
          class="com.sap.adt.refactoring.ui.internal.textsymbols.CreateTextSymbolHandler"
          name="Create Text Symbol">
    </quickfixHandler>
    <quickfixHandler
          resourceUriPattern=".*/create_text_symbol_literal"
          class="com.sap.adt.refactoring.ui.internal.textsymbols.CreateTextSymbolHandler"
          name="Create Text Symbol Literal">
    </quickfixHandler>
    <quickfixHandler
          class="com.sap.adt.refactoring.ui.internal.textsymbols.CreateTextSymbolHandler"
          name="Edit Text Symbol"
          resourceUriPattern=".*/edit_text_symbol">
    </quickfixHandler>
    <quickfixHandler
          class="com.sap.adt.refactoring.ui.internal.textsymbols.CreateTextSymbolHandler"
          name="Adjust Text Symbol"
          resourceUriPattern=".*/adjust_text_symbol">
    </quickfixHandler>
    <quickfixHandler
          class="com.sap.adt.refactoring.ui.internal.extractinterface.ExtractInterfaceHandler"
          name="Extract Interface"
          resourceUriPattern=".*/extract_local_interface">
    </quickfixHandler>
    <quickfixHandler
          class="com.sap.adt.refactoring.ui.internal.extractinterface.ExtractInterfaceHandler"
          name="Extract Superclass"
          resourceUriPattern=".*/extract_local_superclass">
    </quickfixHandler>
  </extension>
  <extension
       point="com.sap.adt.tools.core.ui.quickAssistProcessors">
    <quickAssistProcessor
          class="com.sap.adt.refactoring.ui.internal.quickfix.AbapQuickfixProcessor"
          name="ABAP Quick Assist Processor">
    </quickAssistProcessor>
  </extension>
 
  <!-- Views -->
  <extension
       point="org.eclipse.ui.views">
    <view
          allowMultiple="false"
          category="com.sap.adt.views"
          class="com.sap.adt.refactoring.ui.internal.views.RefactoringView"
          icon="icons/etool/smartmode_co.png"
          id="com.sap.adt.refactoring.ui.RefactoringView"
          name="%view.name.refactoring_xtit"
          restorable="true">
    </view>
  </extension>
 
  <!-- Actions -->
  <extension
       id="com.sap.adt.ProjectExplorer"
       point="org.eclipse.ui.navigator.viewer">
    <viewerActionBinding
          viewerId="org.eclipse.ui.navigator.ProjectExplorer">
       <includes>
          <actionExtension
                pattern="com.sap.adt.tools.core.ui.renameAction">
          </actionExtension>
       </includes>
    </viewerActionBinding>
  </extension>
 
  <extension
       point="org.eclipse.ui.navigator.navigatorContent">
    <actionProvider
          class="com.sap.adt.refactoring.ui.internal.op.rename.RenameActionProvider"
          id="com.sap.adt.tools.core.ui.renameAction"
          overrides="org.eclipse.ui.navigator.resources.actions.RefactorActions"
          priority="higher">
       <enablement>
          <and>
             <and>
                <adapt
                      type="com.sap.adt.tools.core.IAdtObjectReference">
                   <not>
                      <test
                            property="com.sap.adt.tools.core.hasUriFragment"
                            value="true">
                      </test>
                   </not>
                </adapt>
                <test
                      property="com.sap.adt.tools.core.objectTypes.hasType"
                      value="com.sap.adt.oo.classes, com.sap.adt.oo.interfaces">
                </test>
             </and>
          </and>
       </enablement>
    </actionProvider>
  </extension>

  <!-- Marker Resolution -->
  <extension
       point="org.eclipse.ui.ide.markerResolution">
    <markerResolutionGenerator
          class="com.sap.adt.refactoring.ui.internal.quickfix.marker.AbapQuickfixMarkerResolutionGenerator"
          markerType="com.sap.adt.tools.core.abapSyntaxCheckProblem">
    </markerResolutionGenerator>
    <markerResolutionGenerator
          class="com.sap.adt.refactoring.ui.internal.quickfix.marker.AbapQuickfixMarkerResolutionGenerator"
          markerType="com.sap.adt.activation.abapActivationProblem">
    </markerResolutionGenerator>
     <markerResolutionGenerator
          class="com.sap.adt.refactoring.ui.internal.quickfix.marker.AbapQuickfixMarkerResolutionGenerator"
          markerType="com.sap.adt.atc.finding.v2">
    </markerResolutionGenerator>   
  </extension>

  <!-- Preferences -->
  <extension
       point="org.eclipse.core.runtime.preferences">
    <initializer
           class="com.sap.adt.refactoring.ui.internal.preference.RefactoringPreferenceInitializer">
    </initializer>
  </extension>

  <extension
       point="org.eclipse.ui.preferencePages">
    <page
          category="com.sap.adt.tools.abapsource.ui.preferencePages.sourceEditors"
          class="com.sap.adt.refactoring.ui.internal.preference.QuickfixPreferencePage"
          id="com.sap.adt.refactoring.ui.preferencePages.quickfix"
          name="%page.name.quickfix_xgrp">
      <keywordReference
            id="com.sap.adt.refactoring.ui.keywords.refactoring">
      </keywordReference>
    </page>
  </extension>
 
  <extension
	   point="org.eclipse.ui.keywords">
	<keyword
	     id="com.sap.adt.refactoring.ui.keywords.refactoring"
	     label="%keywords.refactoring_xmsg">
    </keyword>
  </extension>
 
</plugin>
